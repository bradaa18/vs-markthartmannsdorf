            </Button>
            <Button fx:id="btnClass" maxHeight="1.7976931348623157E308" mnemonicParsing="false" text="Lehrer" />
         </children>
         <BorderPane.margin>
            <Insets left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </HBox>
   </top>
   <left>
      <BorderPane prefHeight="351.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <center>
            <ImageView fitHeight="284.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="TOP_CENTER" />
         </center>

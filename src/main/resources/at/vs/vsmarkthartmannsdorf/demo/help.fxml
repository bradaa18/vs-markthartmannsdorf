            <Button fx:id="btnTimetable" maxHeight="1.7976931348623157E308" mnemonicParsing="false" text="Stundenplan">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="btnTeacher" maxHeight="1.7976931348623157E308" mnemonicParsing="false" text="Klasse">
